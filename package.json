{
  "name": "YIME",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env VUE_APP_MODE=dev vue-cli-service serve",
    "build": "cross-env VUE_APP_MODE=dev vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:serve": "cross-env VUE_APP_MODE=dev node build/index.js && cross-env VUE_APP_MODE=dev vue-cli-service electron:serve",
    "electron:build_dev": "cross-env VUE_APP_MODE=dev node build/index.js && cross-env VUE_APP_MODE=dev vue-cli-service electron:build -m",
    "electron:build_yime": "cross-env VUE_APP_MODE=yime node build/index.js && cross-env VUE_APP_MODE=yime vue-cli-service electron:build -mw",
    "electron:build_duoliao": "cross-env VUE_APP_MODE=duoliao node build/index.js && cross-env VUE_APP_MODE=duoliao vue-cli-service electron:build",
    "electron:build_momo": "cross-env VUE_APP_MODE=momo node build/index.js && cross-env VUE_APP_MODE=momo vue-cli-service electron:build",
    "iconfont-create": "npx iconfont-create && npm run lint",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "proto": "pbjs -t json-module -w commonjs -o src/protoBuild/proto.js  src/proto/*.proto"
  },
  "main": "background.js",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.3.3",
    "@rongcloud/imlib-next": "^5.1.0",
    "@rongcloud/plugin-call": "^5.0.7",
    "@rongcloud/plugin-rtc": "^5.3.0",
    "ali-oss": "^6.16.0",
    "axios": "^0.24.0",
    "benz-amr-recorder": "^1.1.3",
    "clipboard": "^2.0.8",
    "core-js": "^3.6.5",
    "electron-updater": "^5.2.1",
    "electron-windows-badge": "^1.1.0",
    "lottie-web": "^5.8.1",
    "moment": "^2.29.1",
    "node-machine-id": "^1.1.12",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cnchar-all": "^3.0.4",
    "cross-env": "^7.0.3",
    "electron": "^16.0.5",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "js-md5": "^0.7.3",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.15",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.2.1",
    "protobufjs": "^6.11.2",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "^2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}